int Solution::lis(const vector<int> &arr) {
    int n=arr.size();
    vector<int> lis(n,1);
    for(int i=1;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(arr[i]>arr[j] && lis[i]<=lis[j])
                lis[i]=lis[j]+1;
        }
    }
    int ma=0;
    for(int i=0;i<n;i++)
    {
        if(ma<lis[i])
            ma=lis[i];
    }
    return ma;
}
